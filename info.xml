<?xml version="1.0"?>
<extension key="eu.commondo.rfmanalysis" type="module">
  <file>rfmanalysis</file>
  <name>Customer RFM Analysis Tool</name>
  <description>This extension will perform RFM analysis on your contacts. RFM analysis is recalculated once a day. RFM analysis results for each customer are showed on Summary page of each customer in RFM analysis tab.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>CommCivi</author>
    <email>patrik.kranjcec@commondo.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/CommCivi/eu.commondo.rfmanalysis</url>
    <url desc="Documentation">https://github.com/CommCivi/eu.commondo.rfmanalysis</url>
    <url desc="Support">https://github.com/CommCivi/eu.commondo.rfmanalysis</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-03-27</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Rfmanalysis</namespace>
  </civix>
</extension>
